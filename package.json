{
  "name": "@rlvt/cloud-storage",
  "version": "0.5.0",
  "description": "A simple wrapper for managing blob storage on AWS S3 and Google Storage with a unified API",
  "main": "index.js",
  "engines": {
    "node": ">=5.0.0"
  },
  "scripts": {
    "pretest": "standard",
    "test": "nyc --all tape 'test/unit/*.test.js' | tap-spec",
    "posttest": "nyc report --reporter=lcov",
    "e2e": "tape 'test/e2e/*.e2e.js' | tap-spec",
    "2npm": "publish"
  },
  "nyc": {
    "exclude": [
      "test",
      "coverage",
      "node_modules"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kiwup/cloud-storage.git"
  },
  "keywords": [
    "s3",
    "google storage",
    "aws",
    "gcp"
  ],
  "author": "Reelevant Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kiwup/cloud-storage/issues"
  },
  "homepage": "https://github.com/Kiwup/cloud-storage#readme",
  "dependencies": {
    "@google-cloud/storage": "1.7.0",
    "aws-sdk": "2.285.1",
    "bluebird": "3.5.0",
    "fast-crc32c": "1.0.4",
    "is-stream": "1.1.0"
  },
  "devDependencies": {
    "nyc": "11.2.1",
    "proxyquire": "1.8.0",
    "publish": "0.6.0",
    "request": "2.82.0",
    "request-promise": "4.2.1",
    "standard": "10.0.3",
    "tap-spec": "4.1.1",
    "tape": "4.8.0"
  }
}
